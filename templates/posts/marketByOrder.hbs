<h3> Market By Order</h3>
<div class="row">
    <div class="col-lg-6">
        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th colspan="2">
                        Buy
                    </th>
                </tr>
                <tr>
                    <th>Volume</th>
                    <th>Price</th>
                </tr>
            </thead>

            <tr></tr>

            {{#each order in buys}}
            <tr>
                <td>
                    {{order.volume}}
                </td>
                <td>
                    {{order.price}}
                </td>

            </tr>
            {{/each}}
        </table>
    </div>
    <div class="col-lg-6">

        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th colspan="2">
                        Sell
                    </th>
                </tr>
                <tr>
                    <th>Volume</th>
                    <th>Price</th>
                </tr>
            </thead>

            <tr></tr>

            {{#each sells}}
            <tr>
                <th>
                    {{volume}}
                </th>
                <th>
                    {{price}}
                </th>

            </tr>
            {{/each}}


        </table>
    </div>
</div>
<h3> Market By Price</h3>
<div class="row">
    <div class="col-lg-6">

        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th colspan="3">
                        Buy
                    </th>
                </tr>
                <tr>
                    <th>#</th>
                    <th>Volume</th>
                    <th>Price</th>
                </tr>
            </thead>
            {{#each order in buys}}
            <tr>
                <td>{{@index}}</td>
                <td>{{order.volume}}</td>
                <td>{{order.price}}</td>
            </tr>
            {{/each}}
        </table>
    </div>
    <div class="col-lg-6">

        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th colspan="3">
                        Sell
                    </th>
                </tr>
                <tr>
                    <th>#</th>
                    <th>Volume</th>
                    <th>Price</th>
                </tr>
            </thead>
            {{#each order in sells}}
            <tr>
                <td>{{@index}}</td>
                <td>{{order.volume}}</td>
                <td>{{order.price}}</td>
            </tr>
            {{/each}}
        </table>
    </div>
</div>